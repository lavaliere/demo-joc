<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>

  <parent>
    <groupId>com.cloudbees.operations-center.server</groupId>
    <artifactId>operations-center-parent-server</artifactId>
    <version>1.6.7</version>
    <relativePath>../../operations-center-parent-server/pom.xml</relativePath>
  </parent>
  <artifactId>operations-center-analytics</artifactId>
  <version>1.6</version>
  <packaging>hpi</packaging>

  <name>Operations Center Analytics</name>

  <properties>
    <java.level>6</java.level>
  </properties>

  <dependencies>
    <dependency>
      <groupId>com.cloudbees.operations-center.server</groupId>
      <artifactId>operations-center-server</artifactId>
      <version>1.6</version>
    </dependency>
    <dependency>
      <groupId>com.cloudbees.operations-center.server</groupId>
      <artifactId>operations-center-clusterops</artifactId>
      <version>1.6</version>
    </dependency>
    <dependency>
      <groupId>com.cloudbees.operations-center.client</groupId>
      <artifactId>operations-center-analytics-reporter</artifactId>
      <version>1.6</version>
    </dependency>
    <dependency>
      <groupId>com.cloudbees.operations-center.server</groupId>
      <artifactId>operations-center-analytics-viewer</artifactId>
      <version>1.6</version>
    </dependency>
    <dependency>
      <groupId>com.cloudbees.operations-center.server</groupId>
      <artifactId>operations-center-analytics-dashboards</artifactId>
      <version>1.6</version>
    </dependency>
    <dependency>
      <groupId>org.jenkins-ci.plugins</groupId>
      <artifactId>async-http-client</artifactId>
      <version>1.7.8</version>
    </dependency>
    <dependency>
      <groupId>com.cloudbees.analytics.common</groupId>
      <artifactId>cloudbees-analytics-feeder-common</artifactId>
      <version>1.6</version>
      <exclusions>
        <exclusion>
          <!-- Don't include conflicting Guava version -->
          <groupId>org.sonatype.sisu</groupId>
          <artifactId>sisu-guava</artifactId>
        </exclusion>
        <exclusion>
          <!-- Don't include conflicting Guice version -->
          <groupId>com.google.inject</groupId>
          <artifactId>guice</artifactId>
        </exclusion>
        <exclusion>
          <!-- we get this from cloudbees-analytics-base -->
          <groupId>com.cloudbees.analytics.common</groupId>
          <artifactId>cloudbees-analytics-base-spi</artifactId>
        </exclusion>
        <exclusion>
          <groupId>com.ning</groupId>
          <artifactId>async-http-client</artifactId>
        </exclusion>
      </exclusions>
    </dependency>
    <dependency>
      <groupId>com.cloudbees</groupId>
      <artifactId>extensibility</artifactId>
      <version>1.5</version>
      <exclusions>
        <exclusion><!-- Don't collide with Jenkins' own guava -->
          <groupId>org.sonatype.sisu</groupId>
          <artifactId>sisu-guava</artifactId>
        </exclusion>
        <exclusion><!-- Don't collide with Jenkins' own guice -->
          <groupId>org.sonatype.sisu</groupId>
          <artifactId>sisu-guice</artifactId>
        </exclusion>
      </exclusions>
    </dependency>
    <dependency>
      <groupId>com.cloudbees.operations-center.server</groupId>
      <artifactId>operations-center-elasticsearch-provider</artifactId>
      <version>1.6</version>
    </dependency>
    <dependency>
      <groupId>com.cloudbees.analytics.common</groupId>
      <artifactId>cloudbees-analytics-feeder-common</artifactId>
      <version>1.6</version>
      <exclusions>
        <exclusion>
          <!-- Don't include conflicting Guava version -->
          <groupId>org.sonatype.sisu</groupId>
          <artifactId>sisu-guava</artifactId>
        </exclusion>
        <exclusion>
          <!-- Don't include conflicting Guice version -->
          <groupId>com.google.inject</groupId>
          <artifactId>guice</artifactId>
        </exclusion>
        <exclusion>
          <!-- we get this from cloudbees-analytics-base -->
          <groupId>com.cloudbees.analytics.common</groupId>
          <artifactId>cloudbees-analytics-base-spi</artifactId>
        </exclusion>
      </exclusions>
    </dependency>
    <dependency>
      <groupId>com.cloudbees</groupId>
      <artifactId>extensibility</artifactId>
      <version>1.5</version>
      <exclusions>
        <exclusion><!-- Don't collide with Jenkins' own guava -->
          <groupId>org.sonatype.sisu</groupId>
          <artifactId>sisu-guava</artifactId>
        </exclusion>
        <exclusion><!-- Don't collide with Jenkins' own guice -->
          <groupId>org.sonatype.sisu</groupId>
          <artifactId>sisu-guice</artifactId>
        </exclusion>
      </exclusions>
    </dependency>
    <dependency>
      <groupId>org.apache.lucene</groupId>
      <artifactId>lucene-test-framework</artifactId>
      <version>4.9.1</version>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>org.elasticsearch</groupId>
      <artifactId>elasticsearch</artifactId>
      <version>1.3.5</version>
      <type>test-jar</type>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>org.elasticsearch</groupId>
      <artifactId>elasticsearch</artifactId>
      <version>1.3.5</version>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>com.cloudbees.operations-center.server</groupId>
      <artifactId>operations-center-embedded-elasticsearch</artifactId>
      <version>1.6-1.3.5.0</version>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>log4j</groupId>
      <artifactId>log4j</artifactId>
      <version>1.2.17</version>
      <scope>test</scope>
    </dependency>
  </dependencies>
  <build>
    <plugins>
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>findbugs-maven-plugin</artifactId>
      </plugin>
      <plugin>
        <groupId>org.jenkins-ci.tools</groupId>
        <artifactId>maven-hpi-plugin</artifactId>
        <extensions>true</extensions>
        <configuration>
          <disabledTestInjection>true</disabledTestInjection>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-enforcer-plugin</artifactId>
        <executions>
          <execution>
            <id>display-info</id>
            <configuration>
              <rules>
                <enforceBytecodeVersion>
                  <ignoredScopes>
                    <ignoredScope>test</ignoredScope>
                  </ignoredScopes>
                </enforceBytecodeVersion>
              </rules>
            </configuration>
          </execution>
        </executions>
        <dependencies>
          <dependency>
            <groupId>org.codehaus.mojo</groupId>
            <artifactId>extra-enforcer-rules</artifactId>
            <version>1.0-beta-3</version>
          </dependency>
        </dependencies>
      </plugin>
    </plugins>
  </build>

  <scm>
    <tag>operations-center-analytics-project-1.6</tag>
  </scm>
</project>
